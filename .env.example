# Your Base wallet address (USDC receiver)
ADDRESS=0x0000000000000000000000000000000000000000

# Coinbase Developer Platform API keys (required for mainnet facilitator)
CDP_API_KEY_ID=your_cdp_api_key_id_here
CDP_API_KEY_SECRET=your_cdp_api_key_secret_here

# OpenRouter API key (required)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Default LLM model (defaults to openai/gpt-3.5-turbo)
OPENROUTER_MODEL=openai/gpt-3.5-turbo

# Optional: Server port (defaults to 4021)
PORT=4021

# Optional: OpenRouter HTTP Referer header
OPENROUTER_HTTP_REFERER=

# Optional: OpenRouter X-Title header
OPENROUTER_X_TITLE=
